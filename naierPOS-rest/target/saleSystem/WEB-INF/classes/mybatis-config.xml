<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration
PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
	<typeAliases>
		<!--实体类别名 -->
	     <typeAlias type="com.joker.common.model.SaleOrg" alias="saleOrg" />
	     <typeAlias type="com.joker.common.model.SaleUser" alias="saleUser" />
	</typeAliases>

    <plugins>
        <plugin interceptor="com.joker.core.interceptor.PageInterceptor">
            <property name="databaseType" value="mysql"/>
        </plugin>
    </plugins>
    
</configuration>